# Rename this file to .env.local and fill in the values.

# Google Maps (pÄƒstrat pentru client autocomplete)
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=YOUR_GOOGLE_MAPS_API_KEY
# Google Maps (server) - used for Distance Matrix/Directions on server-side
GOOGLE_MAPS_API_KEY_SERVER=

# Mapbox (pentru admin + driver maps)
NEXT_PUBLIC_MAPBOX_ACCESS_TOKEN=pk.your_mapbox_token_here

# Supabase NEW DB - VantageLane (Auth + DB + Storage)
NEXT_PUBLIC_SUPABASE_URL=https://fmeonuvmlopkutbjejlo.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here
# Service role key is used ONLY on server (never expose on client)
SUPABASE_SERVICE_ROLE_KEY=

# Database (legacy Prisma, optional during migration)
DATABASE_URL=

# Auth secret for legacy custom session (fallback only)
NEXTAUTH_SECRET=

# Stripe (to be added at checkout stage)
STRIPE_SECRET_KEY=
STRIPE_WEBHOOK_SECRET=

# Email Configuration
SMTP_HOST=
SMTP_PORT=587
SMTP_USER_BOOKINGS=
SMTP_PASS_BOOKINGS=
EMAIL_TO_ADMIN=

# Force strict DB reading (no MOCK operators)
DISABLE_OPERATOR_MOCKS=true
