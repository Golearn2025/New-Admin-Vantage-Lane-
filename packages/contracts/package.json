{
  "name": "@vantage-lane/contracts",
  "version": "0.1.0",
  "description": "TypeScript types and API contracts (CardSpec, ChartSpec, ErrorPayload)",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "typescript",
    "types",
    "contracts",
    "api"
  ],
  "license": "MIT"
}
