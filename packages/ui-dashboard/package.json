{
  "name": "@vantage-lane/ui-dashboard",
  "version": "0.1.0",
  "description": "Premium dashboard components - MetricCard, Charts (BarBasic, LineChart, etc)",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./styles": "./dist/index.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "recharts": "^2.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "react",
    "dashboard",
    "charts",
    "metrics",
    "recharts",
    "premium"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "restricted"
  }
}
